@{
    @model IEnumerable<PharmaSafeChain.Models.UserManagementModels.User_Information_Model>

    <!DOCTYPE html>
    <html>
    <head>
        <title>User Identity Information</title>
    </head>
    <body>
        <h1>Developer Information</h1>
        <table>
            <tbody>
                @foreach (var user in ViewBag.UserIdentity)
                {
                    <tr>
                        <td>@user.FirstName @user.LastName - @user.Email - @user.PhoneNumber </td>
                    </tr>
                }
            </tbody>
        </table>
    </body>
    </html>

}
